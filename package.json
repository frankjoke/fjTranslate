{
  "name": "translatedictionary",
  "version": "0.1.0",
  "description": "Translator for words.js files using Google or Yandex translate",
  "author": {
    "name": "frankjoke",
    "email": "frankjoke@hotmail.com"
  },
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "upgrade-interactive": "npm-check --update"
  },
  "main": "background.js",
  "dependencies": {
    "browser-util-inspect": "^0.2.0",
    "file-saver": "^2.0.2",
    "material-design-icons-iconfont": "^5.0.1",
    "typeface-roboto": "0.0.75",
    "vue": "^2.6.11",
    "vue-clipboard2": "^0.3.1",
    "vuetify": "^2.2.28",
    "axios": "^0.19.2",
    "yandex-translator": "^1.0.1"
  },
  "devDependencies": {
    "@mdi/font": "^5.2.45",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/devtools": "^5.3.3",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.6.5",
    "electron": "^9.0.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "npm-check": "^5.9.2",
    "prettier": "^2.0.5",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-electron-builder": "^1.4.6",
    "vue-cli-plugin-vuetify": "^2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.4.3"
  },
  "translatedictionary": {
    "devLocale": "en",
    "allLocales": [
      "en",
      "de",
      "ru",
      "pt",
      "nl",
      "fr",
      "it",
      "es",
      "pl",
      "zh-cn"
    ],
    "locales": [
      "de",
      "ru",
      "pt",
      "nl",
      "fr",
      "it",
      "es",
      "pl",
      "zh-cn"
    ],
    "useYandex": false,
    "globalWordsFile": {
      "type": "Javascript",
      "skipAtStart": "{",
      "skipAfterEnd": "}",
      "addAtStart": "!",
      "addAtEnd": "!"
    },
    "editWordsFile": {
      "type": "Javascript",
      "skipAtStart": "{",
      "skipAfterEnd": "}",
      "addAtStart": "!",
      "addAtEnd": "!"
    },
    "yandexKey": ""
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "double": 0,
      "isable-next-line": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
